{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "connecting-vision": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/connecting-vision",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/templatemo-diagoona.css",
              "src/assets/fontawesome/css/all.min.css"
            ],
            "scripts": [
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery-3.4.1.min.js",
              "src/assets/js/jquery.backstretch.min.js",
              "src/assets/js/templatemo-script.js"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/assets/img",
              "src/assets/img/fundalBlue_jpeg.jpg",
              "src/assets/img/klipartz.com (28).png",
              "src/assets/img/715066_b08af277a1b948b49c5f45486562bcb6~mv2.jpg",
              "src/assets/img/valerie-v-zSrksQgp4W0-unsplash.jpg",
              "src/assets/img/scott-blake-M5wPcXukAVM-unsplash.jpg",
              "src/assets/img/jared-brashier-duNHkmSkW6M-unsplash.jpg",
              "src/assets/img/max-bottinger-Gup8MCvSsf0-unsplash.jpg",
              "src/assets/img/FMB-Sketch.jpg",
              "src/assets/img/scott-graham-OQMZwNd3ThU-unsplash.jpg"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "connecting-vision:build:production"
            },
            "development": {
              "buildTarget": "connecting-vision:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "connecting-vision:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "version": 2
          },
          "configurations": {
            "production": {
              "buildTarget": "connecting-vision:build:production",
              "serveTarget": "connecting-vision:serve:production"
            },
            "development": {
              "buildTarget": "connecting-vision:build:development",
              "serveTarget": "connecting-vision:serve:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}
